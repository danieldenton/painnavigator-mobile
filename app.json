{
  "expo": {
    "name": "PainNavigator",
    "slug": "PainNavigator",
    "version": "1.4.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "painnavigator",
            "project": "painnavigator",
            "authToken": "eda9fdc4ea6549e49c310313a96e8a62605f19c8e3364174a8b1d3a9571aea24"
          }
        }
      ]
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F8FB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stephenavocado.PainNavigator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "sentry-expo"
    ],
    "extra": {
      "eas": {
        "projectId": "d9441de4-82db-4481-a4d1-7d868863b825"
      }
    }
  }
}
