{
  "expo": {
    "name": "PainNavigator",
    "slug": "PainNavigator",
    "version": "2.0.8",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F8FB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/30078975-de73-452b-98f9-9cdcc505c8f3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.stephenavocado.PainNavigator",
      "buildNumber": "111",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSMicrophoneUsageDescription": "Allow PainNavigator to access your microphone.",
        "NSPrivacyAccessedAPICategoryDiskSpace": "Allow PainNavigator to use disk space for authentication to keep you logged in until you choose to logout.",
        "NSPrivacyAccessedAPICategoryFileTimestamp": "Allow PainNavigator to timestamp your progress to update your program daily."
      }
    },
    "android": {
      "package": "com.PainNavigator",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5CCBB8"
      },
      "blockedPermissions": [
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "icon": "./assets/icon.png",
      "versionCode": 107,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/adaptive-icon.png",
          "color": "#37b29d"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow PainNavigator to access your microphone."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "painnavigator",
          "project": "painnavigator"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "30078975-de73-452b-98f9-9cdcc505c8f3"
      }
    },
    "owner": "painnavigator",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
