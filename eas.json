{
  "cli": {
    "version": ">= 3.1.1"
  },
  "build": {
    "preview": {
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "development"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "process.env.API_URL",
        "AMPLITUDE_API_KEY": "process.env.AMPLITUDE_API_KEY"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}